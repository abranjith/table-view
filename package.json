{
  "name": "simple-file-viewer",
  "displayName": "Simple File Viewer",
  "description": "View delimited text files (such as CSV) in a clear, readable table format",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.103.0"
  },
  "publisher": "ab-ranjith",
  "license": "SEE LICENSE IN LICENSE",
  "categories": [
    "Data Science",
    "Visualization"
  ],
  "author": {
    "name": "ranjith"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/abranjith/table-view.git"
  },
  "homepage": "https://github.com/abranjith/table-view/blob/main/README.md",
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "fileViewer.openAsTable",
        "title": "Open as Table",
        "category": "File Viewer"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "fileViewer.openAsTable",
          "when": "resourceScheme == file",
          "group": "navigation"
        }
      ],
      "editor/title": [
        {
          "command": "fileViewer.openAsTable",
          "when": "resourceScheme == file",
          "group": "navigation"
        }
      ]
    },
    "customEditors": [
      {
        "viewType": "fileViewer.fileEditor",
        "displayName": "Table View",
        "selector": [
          {
            "filenamePattern": "*"
          }
        ],
        "priority": "option"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.103.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2",
    "ts-loader": "^9.5.2",
    "webpack": "^5.101.0",
    "webpack-cli": "^6.0.1",
    "copy-webpack-plugin": "^12.0.2",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
